{
  "name": "sort-lines",
  "main": "./lib/sort-lines",
  "version": "0.14.0",
  "description": "Sorts your lines. Never gets tired.",
  "activationCommands": {
    "atom-text-editor": [
      "sort-lines:sort",
      "sort-lines:reverse-sort",
      "sort-lines:unique",
      "sort-lines:case-insensitive-sort",
      "sort-lines:natural",
      "sort-lines:yaml-aware#roaming-mode",
      "sort-lines:yaml-aware#steady-mode"
    ]
  },
  "repository": "https://github.com/atom/sort-lines",
  "license": "MIT",
  "engines": {
    "atom": ">=1.4.0 <2.0.0"
  },
  "dependencies": {
    "yaml-js": "~0.1.3"
  }
}
